class Animal():
	def __init__(self,name,age):
		self.name = name
		self.age =age
	def sleep(self):
		print self.name + " of " + str(self.age) + " is sleeping "
	def eat (self,food):
		print self.name + " of " + str(self.age) + " is eating " + food 
x = Animal("MEET" , 11 )
x.sleep()
x.eat("apple")
y = Animal("Cat" , 2 )
y.sleep()
y.eat("pizza") 
